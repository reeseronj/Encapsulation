class Book:
    def __init__(self, title, author):
        self.__title = title  
        self.__author = author  

    def get_title(self):
        return self.__title  

    def get_author(self):
        return self.__author  

my_book = Book("1984", "George Orwell")
print("Title:", my_book.get_title())   
print("Author:", my_book.get_author())  
